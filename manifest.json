{
  "manifest_version": 3,
  "name": "网站时间统计",
  "version": "0.2",
  "description": "统计Chrome在整个打开期间每个网站的独立停留时间",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "idle",
    "alarms"
  ]
}
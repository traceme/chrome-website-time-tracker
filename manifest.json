{
  "manifest_version": 3,
  "name": "网站时间统计",
  "version": "0.1",
  "description": "统计每天在各个网站上花费的时间",
  "action": {
    "default_popup": "popup.html",
    "default_popup_width": 800,  // 设置弹出窗口的宽度
    "default_popup_height": 600  // 设置弹出窗口的高度
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}